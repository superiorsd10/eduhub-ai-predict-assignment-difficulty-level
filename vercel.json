{
    "version": 1,
    "public": false,
    "devCommand": "uvicorn app:app --host 0.0.0.0 --port 80",
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}